function RUI_StatusDoesNotInvokeOnStatusApply()
  return StatusDoesNotInvokeOnStatusApply()
      | StatusId('RUI_FROST_HAT_PLACEHOLDER_DO_NOT_REMOVE')
      | StatusId('RUI_FROST_MARKOHESHKIR_PLACEHOLDER')
      | StatusId('RUI_NOXIOUS_FUMES_GLOVES_PLACEHOLDER')
      | StatusId('RUI_NOXIOUS_FUMES_MARKOHESHKIR_PLACEHOLDER')
      | StatusId('RUI_POISON_MARKOHESHKIR_PLACEHOLDER')
      | StatusId('RUI_REVERBERATION_AMULET_PLACEHOLDER')
      | StatusId('RUI_REVERBERATION_BOOTS_PLACEHOLDER')
      | StatusId('RUI_REVERBERATION_GLOVES_PLACEHOLDER')
      | StatusId('RUI_REVERBERATION_MARKOHESHKIR_PLACEHOLDER')
      | StatusId('RUI_SNOWBURST_RING_PLACEHOLDER')
end
